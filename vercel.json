{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/.*",
      "dest": "/public/$1",
      "headers": { "Cache-Control": "public, max-age=31536000" }
    },
    {
      "src": "/images/.*",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ]
}
